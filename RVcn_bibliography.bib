@article{Beaudequin2021,
  title = {A Novel, Complex Systems Approach to Modelling Risk of Psychological Distress in Young Adolescents},
  author = {Beaudequin, Denise and Schwenn, Paul and McLoughlin, Larisa T. and Parker, Marcella and Boyes, Amanda and Simcock, Gabrielle and Lagopoulos, Jim and Hermens, Daniel F.},
  year = {2021},
  month = may,
  journal = {Scientific Reports},
