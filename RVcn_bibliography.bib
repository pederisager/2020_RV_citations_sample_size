  shorttitle = {Registered {{Replication Report}}},
  author = {Eerland, A. and Sherrill, A. M. and Magliano, J. P. and Zwaan, R. A. and Arnal, J. D. and Aucoin, P. and Berger, S. A. and Birt, A. R. and Capezza, N. and Carlucci, M. and Crocker, C. and Ferretti, T. R. and Kibbe, M. R. and Knepp, M. M. and Kurby, C. A. and Melcher, J. M. and Michael, S. W. and Poirier, C. and Prenoveau, J. M.},
