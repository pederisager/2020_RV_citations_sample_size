@article{hikosaka2008,
  title = {New Insights on the Subcortical Representation of Reward},
  author = {Hikosaka, Okihide and {Bromberg-Martin}, Ethan and Hong, Simon and Matsumoto, Masayuki},
  year = {2008},
  journal = {Current Opinion in Neurobiology},
  volume = {18},
