  author = {Coles, Nicholas A. and March, David Scott and {Marmolejo-Ramos}, Fernando and Banaruee, Hassan and Butcher, Natalie and Cavallet, Mikael and Dagaev, Nikolay and Eaves, Daniel and Foroni, Francesco and Gorbunova, Elena and Gygax, Pascal and IJzerman, Hans and Hinojosa, Jos{\'e} Antonio and Ikeda, Ayumi and {Khatin-Zadeh}, Omid and Larsen, Jeff T. and {\"O}zdo{\u g}ru, Asil Ali and Parzuchowski, Michal and {Rodriguez-Medina}, David A. and {Ruiz-Fernandez}, Susana and Som, Bidisha and Suarez, Isabel and Trujillo, Natalia and Trujillo, Sandra and {van der Zee}, Tim and {Villalba-Garc{\'i}a}, Cristina and Willis, Megan and Yamada, Yuki and Aczel, Balazs and Hajdu, Nandor and {Basnight-Brown}, Dana and Ellsworth, Phoebe C. and Gaertner, Lowell and Strack, Fritz and Liuzza, Marco Tullio and Marozzi, Marco},
  year = {2019},
  month = feb,
  institution = {{PsyArXiv}},
  doi = {10.31234/osf.io/cvpuw},
  abstract = {The facial feedback hypothesis suggests that an individual's subjective experience of emotion is influenced by their own facial expressions. Researchers, however, currently face conflicting narratives about whether this hypothesis is valid. A large replication effort consistently failed to replicate a seminal demonstration of the facial feedback hypothesis, but meta-analysis suggests the effect is real. To address this uncertainty, a large team of researchers\textemdash some advocates of the facial feedback hypothesis, some critics, and some without strong belief\textemdash collaborated to specify the best ways to test this hypothesis. Two pilot tests of the agreed-upon methodology suggested that smiling could both magnify ongoing feelings of happiness and initiate feelings of happiness in otherwise non-emotional scenarios. Next, 22 labs from 20 countries will examine whether these findings can be replicated.}
}

