@incollection{fiske2012,
  title = {How to Relate to People: {{The}} Extraterrestrial's Guide to {{Homo}} Sapiens},
  booktitle = {Relationship {{Science}}: {{Integrating Evolutionary}}, {{Neuroscience}}, and {{Sociocultural Approaches}}. {{Decade}} of {{Behavior}} 2000\textendash 2010.},
  author = {Fiske, Alan Page and Schubert, Lisa},
  editor = {Gillath, Omri and Adams, Glenn and Kunkel, Adrianne},
  year = {2012},
  pages = {169--195},
  publisher = {{American Psychological Association}},
  address = {{Washington}},
  doi = {10.1037/13489-000},
  abstract = {The first part of this guide for extraterrestrials introduces relational models theory (Fiske, 1991, 2004b) to describe the four elementary ways in which humans organize their interactions. The next section shows how people create, sustain, and modulate interaction, using a distinct conformational system (consyst) for each of the four relational models. The consyst is the semiotic medium in which people represent, communicate, and constitute a relational model. The following section describes how consysts function by cyclically expressing and evoking motives to relate. We next discuss the culturally framed requirements, opportunities, and barriers to forming or sustaining social relationships. Then we discuss how, within or outside these culturally legitimate possibilities, people use the consysts of the relational models to initiate and form a relationship and accept or reject others' invitations. Then we show how, through the proper consyst, people dynamically modulate the intensity of the relationship over time, developing or reducing their commitment to the relationship. Finally, we briefly draw attention to the fact that each consyst has distinct affordances and imposes distinct problems for forming and modulating social relationships, depending on the technologies available, the number of persons, their locations, and related factors. Our focus in this chapter is on culturally oriented psychosocial processes; where there is sufficient evidence we discuss the neurobiological (chemical and anatomical) substrates of the processes. Space does not permit extensive discussion of the deep phylogenetic precursors of the consysts, which we review extensively elsewhere (Fiske, 2011). (PsycINFO Database Record (c) 2013 APA, all rights reserved)}
}

@article{fitousi2006,
  title = {Size Congruity Effects with Two-Digit Numbers: Expanding the Number Line?},
  author = {Fitousi, Daniel and Algom, Daniel},
  year = {2006},
  journal = {Memory \& cognition},
  volume = {34},
  number = {2},
  pages = {445--457},
  issn = {0090-502X (Print)\textbackslash r0090-502X (Linking)},
  doi = {10.3758/BF03193421},
  abstract = {The size congruity effect is the interesting result that comparisons of the sizes of the physical formats in which numerals appear are affected by the numerical magnitudes of the respective numerals. We demonstrated that separating the physical and the numerical attributes in space leaves the effect unchanged. We then applied the spatially separated version to two-digit numerals and showed the effect to be comparable to that obtained with single numerals. We showed further that the effect is sensitive to the relative salience of the numeric and physical dimensions, to the extent that when the latter is the more salient dimension, a reverse effect obtains by which physical size interferes with number comparison. The results can be explained by a relative speed of processing account, but they are also compatible with an attention account that does not appeal to the notion of automaticity.}
}

