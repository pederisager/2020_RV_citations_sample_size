@article{coles2018a,
  title = {The {{Costs}} and {{Benefits}} of {{Replication Studies}}},
  author = {Coles, Nicholas A. and Tiokhin, Leonid and Scheel, Anne and Isager, Peder and Lakens, Daniel},
  year = {2018},
  doi = {10.17605/osf.io/c8akj},
  abstract = {In a summary of recent discussions about the role of direct replications in psychological science, Zwaan, Etz, Lucas, and Donnellan (2017; henceforth ZELD) argue that replications should be more mainstream, and discuss six common objections to direct replication studies. We believe that the debate about the importance of replication research is essentially driven by disagreements about the value of replication studies and the best way to allocate limited resources. We suggest that a decision theory framework (Wald, 1950) can provide a tool for researchers to (a) evaluate costs and benefits in order to determine when replication studies are worthwhile, and (b) specify their assumptions in quantifiable terms, facilitating more productive discussions in which the sources of disagreement about the value of replications can be identified.}
}

