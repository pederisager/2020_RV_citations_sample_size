@article{selleck2015,
  title = {Endogenous {{Opioid Signaling}} in the {{Medial Prefrontal Cortex}} Is {{Required}} for the {{Expression}} of {{Hunger}}-{{Induced Impulsive Action}}},
  author = {Selleck, Ryan A and Lake, Curtis and Estrada, Viridiana and Riederer, Justin and Andrzejewski, Matthew and Sadeghian, Ken and Baldo, Brian A},
  year = {2015},
  journal = {Neuropsychopharmacology},
  volume = {40},
  number = {10},
  pages = {2464--2474},
  issn = {1740-634X (Electronic) 0893-133X (Linking)},
  doi = {10.1038/npp.2015.97},
  abstract = {Opioid transmission and dysregulated prefrontal cortex (PFC) activity\textbackslash nhave both been implicated in the inhibitory-control deficits associated\textbackslash nwith addiction and binge-type eating disorders. What remains unknown,\textbackslash nhowever, is whether endogenous opioid transmission within the PFC\textbackslash nmodulates inhibitory control. Here, we compared intra-PFC opioid\textbackslash nmanipulations with a monoamine manipulation (d-amphetamine), in two\textbackslash nsucrose-reinforced tasks: progressive ratio (PR), which assays the\textbackslash nmotivational value of an incentive, and differential reinforcement of\textbackslash nlow response rates (DRLs), a test of inhibitory control. Intra-PFC\textbackslash nmethylnaloxonium (M-NX, a limited diffusion opioid antagonist) was given\textbackslash nto rats in a `low-drive' condition (2-h food deprivation), and also\textbackslash nafter a motivational shift to a `high-drive' condition (18-h food\textbackslash ndeprivation). Intra-PFC DAMGO (D-\{[\}Ala2, N-MePhe4, Gly-ol]-enkephalin;\textbackslash na mu-opioid agonist) and d-amphetamine were also tested in both tasks,\textbackslash nunder the low-drive condition. Intra-PFC M-NX nearly eliminated\textbackslash nimpulsive action in DRL engendered by hunger, at a dose (1 mu g) that\textbackslash nsignificantly affected neither hunger-induced PR enhancement nor\textbackslash nhyperactivity. At a higher dose (3 mu g), M-NX eliminated impulsive\textbackslash naction and returned PR breakpoint to low-drive levels. Conversely,\textbackslash nintra-PFC DAMGO engendered `high-drive-like' effects: enhancement of PR\textbackslash nand impairment of DRL performance. Intra-PFC d-amphetamine failed to\textbackslash nproduce effects in either task. These results establish that endogenous\textbackslash nPFC opioid transmission is both necessary and sufficient for the\textbackslash nexpression of impulsive action in a high-arousal, high-drive appetitive\textbackslash nstate, and that PFC-based opioid systems enact functionally unique\textbackslash neffects on food impulsivity and motivation relative to PFC-based\textbackslash nmonoamine systems. Opioid antagonists may represent effective treatments\textbackslash nfor a range of psychiatric disorders with impulsivity features.}
}

@article{seo2013,
  title = {Disrupted Ventromedial Prefrontal Function, Alcohol Craving, and Subsequent Relapse Risk.},
  author = {Seo, Dongju and Lacadie, Cheryl M and Tuit, Keri and Hong, Kwang-Ik and Constable, R Todd and Sinha, Rajita},
  year = {2013},
  journal = {JAMA psychiatry},
  volume = {70},
  number = {7},
  pages = {727--39},
  issn = {2168-622x},
  doi = {10.1001/jamapsychiatry.2013.762},
  abstract = {IMPORTANCE: Alcohol dependence is a chronic relapsing illness; stress, alcohol-related cues, and neutral-relaxing states significantly influence craving and relapse risk. However, neural mechanisms underlying the association between these states and alcohol craving and relapse risk remain unclear.\textbackslash n\textbackslash nOBJECTIVES: To identify neural correlates associated with alcohol craving and relapse outcomes in 45 treatment-engaged, 4- to 8-week abstinent alcohol-dependent (AD) patients, and to compare brain responses of 30 demographically matched AD patients and 30 healthy control subjects during stress, alcohol, and neutral-relaxing cues.\textbackslash n\textbackslash nDESIGN: Functional magnetic resonance imaging study while participants were engaging in brief individualized script-driven imagery trials of stress, alcohol cues, and neutral-relaxing scenarios, and a prospective clinical outcome design to assess alcohol relapse 90 days postdischarge from inpatient treatment in the AD group.\textbackslash n\textbackslash nSETTINGS: Inpatient treatment setting in a community mental health center and hospital-based research unit.\textbackslash n\textbackslash nPATIENTS: Forty-five recovering AD patients in inpatient treatment for examining relapse, and 30 healthy control subjects demographically matched to 30 AD patients (subgroup of the relapse sample) for group comparisons.\textbackslash n\textbackslash nINTERVENTION: Twelve-step recovery-based addiction treatment for the patient group.\textbackslash n\textbackslash nMAIN OUTCOMES AND MEASURES: Brain response, alcohol craving, and relapse outcome measures (time to relapse and relapse severity).\textbackslash n\textbackslash nRESULTS: Increased ventromedial prefrontal cortex (vmPFC) and anterior cingulate cortex (ACC) activation during neutral-relaxing trials was correlated with high alcohol cue-induced and stress-induced craving in early recovering AD patients (x = 6, y = 43, z = -6; P {$<$} .01, whole-brain corrected). This vmPFC/ACC hyperactivity significantly predicted subsequent alcohol relapse, with a hazards ratio greater than 8 for increased relapse risk. Additionally, vmPFC/ACC hyperactivation during neutral trials and reduced activity during stress trials were each predictive of greater days of alcohol used after relapse (P {$<$} .01, whole-brain corrected). In contrast, matched control subjects showed the reverse pattern of vmPFC/ACC responses to stress, alcohol cues, and relaxed trials (F = 6.42; P {$<$} .01, whole-brain corrected).\textbackslash n\textbackslash nCONCLUSIONS AND RELEVANCE: Findings indicate that disrupted vmPFC/ACC function plays a role in jeopardizing recovery from alcoholism and may serve as a neural marker to identify those at risk for alcohol relapse.},
  keywords = {Addictive,Addictive: physiopathology,Adolescent,Adult,Alcoholism,Alcoholism: physiopathology,Alcoholism: psychology,Behavior,Case-Control Studies,Corpus Striatum,Corpus Striatum: physiopathology,Cues,Female,Functional Neuroimaging,Functional Neuroimaging: psychology,Gyrus Cinguli,Gyrus Cinguli: physiopathology,Humans,Male,Middle Aged,Prefrontal Cortex,Prefrontal Cortex: physiopathology,Recurrence}
}

