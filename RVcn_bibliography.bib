@article{Bond1998,
  title = {Single-Nucleotide Polymorphism in the Human Mu Opioid Receptor Gene Alters Beta-Endorphin Binding and Activity: Possible Implications for Opiate Addiction.},
  author = {Bond, C and LaForge, K S and Tian, M and Melia, D and Zhang, S and Borg, L and Gong, J and Schluger, J and a Strong, J and Leal, S M and a Tischfield, J and Kreek, M J and Yu, L},
  year = {1998},
