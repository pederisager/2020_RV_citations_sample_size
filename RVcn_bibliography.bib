@article{cahn2006,
  title = {Meditation States and Traits: {{EEG}}, {{ERP}}, and Neuroimaging Studies.},
  author = {Cahn, B Rael and Polich, John},
  year = {2006},
  journal = {Psychological bulletin},
  volume = {132},
