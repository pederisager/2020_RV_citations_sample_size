@article{restle1970,
  title = {Speed of Adding and Comparing Numbers.},
  author = {Restle, Frank},
  year = {1970},
  journal = {Journal of Experimental Psychology},
  volume = {83},
  number = {2},
  pages = {274--278},
  issn = {0022-1015(Print)},
  doi = {10.1037/h0028573},
  abstract = {Presented to 38 undergraduates a series of problems consisting of 2 numbers to be added (A+B) and a comparison number (C) ranging from 13-153. They were to choose the larger, A+B or C, as rapidly as possible. Errors and latency increased with size of numbers, except when A+B and C were on opposite sides of 100. Speed and accuracy increased with the difference between A and B, but were also high when A = B. Errors and latency increased when the absolute difference between A+B and C was relatively small, requiring high accuracy on the part of S. Results were interpreted in terms of an analog operation in which Ss place the magnitudes symbolized by numbers on the number line (an imaginary analog) for manipulating and judging.}
}

