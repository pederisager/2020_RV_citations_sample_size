  author = {Syal, Supriya and Ipser, Jonathan and Terburg, David and Solms, Mark and Panksepp, Jaak and {Malcolm-Smith}, Susan and Bos, Peter A and Montoya, Estrella R and Stein, Dan J and Van Honk, Jack},
