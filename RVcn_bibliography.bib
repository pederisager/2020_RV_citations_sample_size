@article{nathan2009,
  title = {From Taste Hedonics to Motivational Drive: Central {$\mu$}-Opioid Receptors and Binge-Eating Behaviour.},
  author = {Nathan, Pradeep J and Bullmore, Edward T},
  year = {2009},
  journal = {The international journal of neuropsychopharmacology / official scientific journal of the Collegium Internationale Neuropsychopharmacologicum (CINP)},
  volume = {12},
  number = {7},
  pages = {995--1008},
  issn = {1461-1457},
  doi = {10.1017/S146114570900039X},
  abstract = {Endogenous opioids and {$\mu$}-opioid receptors (MORs) have long been implicated in the mechanism of appetite control and, in particular, hedonic processes associated with food evaluation, consumption and orosensory reward processes. In animal models of binge eating, selective MOR antagonists suppress food consumption. In humans, non-selective opioid receptor antagonists reduce hedonic taste preferences and food intake, particularly for palatable foods, and cause short-term weight loss. These effects have been linked to direct stimulation of MORs and modulation of dopamine release within the reward circuitry including the nucleus accumbens. These findings suggest that reduction of MOR-mediated hedonic and motivation processes driving consumption of highly palatable foods may be a promising therapeutic approach and provide a strong rationale for developing safer and more selective MOR antagonists or inverse agonists for disorders of 'appetitive motivation' including obesity and binge-eating disorder.},
  keywords = {binge eating,Binge eating; binge-eating disorder; hedonic; moti,binge-eating disorder,hedonic,m -opioid receptor,motivation}
}

