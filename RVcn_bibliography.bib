@article{bieber2008,
  title = {Retrospective {{Accounts}} of {{Initial Subjective Effects}} of {{Opioids}} in {{Patients Treated}} for {{Pain Who Do}} or {{Do Not Develop Opioid Addiction}}: {{A Pilot Case}}-{{Control Study}}},
