@article{Votaw2021,
  title = {Motives for {{Substance Use}} in {{Daily Life}}: {{A Systematic Review}} of {{Studies Using Ecological Momentary Assessment}}},
  shorttitle = {Motives for {{Substance Use}} in {{Daily Life}}},
  author = {Votaw, Victoria R. and Witkiewitz, Katie},
  year = {2021},
  month = mar,
  journal = {Clinical Psychological Science},
  pages = {2167702620978614},
  publisher = {{SAGE Publications Inc}},
  issn = {2167-7026},
  doi = {10.1177/2167702620978614},
  abstract = {The motivational model of substance use posits that four motive subtypes (coping, enhancement, social, conformity) dynamically interact with contextual factors to affect decisions about substance use. Yet prior studies assessing the motivational model have relied on between-persons, cross-sectional evaluations of trait motives. We systematically reviewed studies using ecological momentary assessment (EMA; N = 64) on motives for substance use to examine methodological features of EMA studies examining the motivational model, support for the motivational model between and within individuals, and associations between trait motives and daily processes. Results of the reviewed studies provide equivocal support for the motivational model and suggest that EMA measures and trait measures of motives might not reflect the same construct. The reviewed body of research indicates that most studies have not examined the momentary and dynamic nature of the motivational model, and more research is needed to inform interventions that address heterogeneous reasons for substance use in daily life.},
  language = {en},
  keywords = {daily diary,ecological momentary assessment,motivation,motive,preregistered,substance use}
}

@article{voytek2016,
  title = {The {{Virtuous Cycle}} of a {{Data Ecosystem}}},
  author = {Voytek, Bradley},
  year = {2016},
  journal = {PLoS Computational Biology},
  volume = {12},
  number = {8},
  pages = {4--9},
  issn = {15537358 (Electronic)},
  doi = {10.1371/journal.pcbi.1005037},
  abstract = {Modern science is creating data at an unprecedented rate, yet most of these data are being discarded.\textbackslash r\textbackslash nRaw scientific data, when they are published at all, are provided in a very limited form.\textbackslash r\textbackslash nLarge, multidimensional datasets\textemdash rich with hidden information\textemdash are reduced to summary\textbackslash r\textbackslash nstatistics filtered through limitations imposed by contemporary methods and technologies, and\textbackslash r\textbackslash nthrough the biased lens of the originating research group. The massive loss of raw data currently\textbackslash r\textbackslash nunderway, and the lack of a system for discovering them, hinders scientific progress. In\textbackslash r\textbackslash nthis Perspective, I argue that our contemporary limited view of the long-term scientific and\textbackslash r\textbackslash nmedical benefits that could be made possible by data sharing masks the benefits for doing so.\textbackslash r\textbackslash nThis, in turn, makes the costs of data sharing seem higher than they are.}
}

@article{vul2009,
  title = {Puzzlingly {{High Correlations}} in {{fMRI Studies}} of {{Emotion}}, {{Personality}}, and {{Social Cognition}}},
  author = {Vul, Edward and Harris, Christine and Winkielman, Piotr and Pashler, Harold},
  year = {2009},
  journal = {Psychological Science},
  volume = {4},
  number = {3},
  pages = {274--290},
  issn = {1745-6916},
  doi = {10.1111/j.1745-6924.2009.01132.x},
  abstract = {ABSTRACT\textemdash Functional magnetic resonance imaging (fMRI)\textbackslash nstudies of emotion, personality, and social cognition have\textbackslash ndrawn much attention in recent years, with high-profile\textbackslash nstudies frequently reporting extremely high (e.g., {$>$}.8) correlations\textbackslash nbetween brain activation and personalitymeasures.\textbackslash nWe show that these correlations are higher than should be\textbackslash nexpected given the (evidently limited) reliability of both fMRI\textbackslash nand personality measures. The high correlations are all the\textbackslash nmore puzzling because method sections rarely contain much\textbackslash ndetail about howthe correlationswere obtained.We surveyed\textbackslash nauthors of 55 articles that reported findings of this kind to\textbackslash ndetermine a few details on how these correlations were\textbackslash ncomputed. More than half acknowledged using a strategy\textbackslash nthat computes separate correlations for individual voxels and\textbackslash nreports means of only those voxels exceeding chosen thresholds.\textbackslash nWe show how this nonindependent analysis inflates\textbackslash ncorrelations while yielding reassuring-looking scattergrams.\textbackslash nThis analysis technique was used to obtain the vast majority\textbackslash nof the implausibly high correlations in our survey sample. In\textbackslash naddition, we argue that, in some cases, other analysis problems\textbackslash nlikely created entirely spurious correlations. We outline\textbackslash nhow the data from these studies could be reanalyzed\textbackslash nwith unbiased methods to provide accurate estimates of the\textbackslash ncorrelations in question and urge authors to perform such\textbackslash nreanalyses. The underlying problems described here appear\textbackslash nto be common in fMRI research of many kinds\textemdash not just in\textbackslash nstudies of emotion, personality, and social cognition.}
}

