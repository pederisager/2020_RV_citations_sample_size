@article{Martin2011,
  title = {The {{Research Excellence Framework}} and the `Impact Agenda': Are We Creating a {{Frankenstein}} Monster?},
  shorttitle = {The {{Research Excellence Framework}} and the `Impact Agenda'},
  author = {Martin, Ben R},
  year = {2011},
