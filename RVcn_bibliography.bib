@incollection{Ryan2018,
  title = {A {{Continuous}}-{{Time Approach}} to {{Intensive Longitudinal Data}}: {{What}}, {{Why}}, and {{How}}?},
  shorttitle = {A {{Continuous}}-{{Time Approach}} to {{Intensive Longitudinal Data}}},
  booktitle = {Continuous {{Time Modeling}} in the {{Behavioral}} and {{Related Sciences}}},
  author = {Ryan, Ois{\'i}n and Kuiper, Rebecca M. and Hamaker, Ellen L.},
  editor = {{van Montfort}, Kees and Oud, Johan H. L. and Voelkle, Manuel C.},
  year = {2018},
  pages = {27--54},
  publisher = {{Springer International Publishing}},
  address = {{Cham}},
  doi = {10.1007/978-3-319-77219-6_2},
  abstract = {The aim of this chapter is to (a) provide a broad didactical treatment of the first-order stochastic differential equation model\textemdash also known as the continuous-time (CT) first-order vector autoregressive (VAR(1)) model\textemdash and (b) argue for and illustrate the potential of this model for the study of psychological processes using intensive longitudinal data. We begin by describing what the CT-VAR(1) model is and how it relates to the more commonly used discrete-time VAR(1) model. Assuming no prior knowledge on the part of the reader, we introduce important concepts for the analysis of dynamic systems, such as stability and fixed points. In addition we examine why applied researchers should take a continuous-time approach to psychological phenomena, focusing on both the practical and conceptual benefits of this approach. Finally, we elucidate how researchers can interpret CT models, describing the direct interpretation of CT model parameters as well as tools such as impulse response functions, vector fields, and lagged parameter plots. To illustrate this methodology, we reanalyze a single-subject experience-sampling dataset with the R package ctsem; for didactical purposes, R code for this analysis is included, and the dataset itself is publicly available.},
  isbn = {978-3-319-77219-6},
  language = {en},
  file = {C\:\\Users\\peder\\Zotero\\storage\\X8NA8V9L\\Ryan et al. - 2018 - A Continuous-Time Approach to Intensive Longitudin.pdf}
}

