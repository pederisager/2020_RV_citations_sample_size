@article{eisenberg1994,
  title = {The Relations of Emotionality and Regulation to Dispositional and Situational Empathy-Related Responding.},
  author = {Eisenberg, N and Fabes, R A and Murphy, B and Karbon, M and Maszk, P and Smith, M and O'Boyle, C and Suh, K},
  year = {1994},
  journal = {Journal of personality and social psychology},
  volume = {66},
  pages = {776--797},
  issn = {1602965854},
  doi = {10.1037/0022-3514.66.4.776},
  abstract = {The purpose of the present study was to examine the prediction of adults' situational and dispositional empathy-related responses from measures of emotionality (emotional intensity and positive and negative affect) and regulation. A multimethod approach including self-reported, facial, and heart rate (HR) responses was used to assess situational vicarious emotional responding; Ss' (and sometimes friends') reports were used to assess the dispositional characteristics. In general, dispositional sympathy, personal distress, and perspective taking exhibited different, conceptually logical patterns of association with indexes of emotionality and regulation. The relations of situational measures of vicarious emotional responding to dispositional emotionality and regulation varied somewhat by type of measure and gender. Findings for facial and HR (for men) measures were primarily for the more evocative empathy-inducing stimulus. In general, the findings provided support for the role of individual differences in emotionality and regulation in empathy-related responding.}
}

