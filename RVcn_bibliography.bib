@article{breese2005,
  title = {Stress {{Enhancement}} of {{Craving During Sobriety}}: {{A Risk}} for {{Relapse}}},
  author = {Breese, George and Chu, K and Dayas. C, V and Funk, D and Knapp, Darin J and Koob, George F and Anh Le, D and O'Dell, E, L and Overstreet, David H and Roberts, J, A and Sinha, R and Valdez, Glenn R and Weiss, Friedbert},
